{
  "name": "azure-edge-locations",
  "version": "0.1.6",
  "description": "List of Azure CDN edge locations",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "mocha test/tests.ts",
    "install-duckdb": "./scripts/install-duckdb.sh",
    "download": "rm -rf temp/ && mkdir -p temp && duckdb < scripts/load-airports.sql",
    "generate": "mkdir -p data && tsx generate.ts",
    "bundle": "rm -rf dist/ src/data/ && mkdir -p src/data && cp data/azure-edge-locations.json src/data && tsc --build src/ && rm -f dist/tsconfig.tsbuildinfo dist/tsconfig.json",
    "build": "npm run download && npm run generate && npm run bundle",
    "prepack": "npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobilg/azure-edge-locations.git"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/data/azure-edge-locations.json"
  ],
  "keywords": [
    "azure",
    "edge",
    "location",
    "codes",
    "lookup"
  ],
  "author": {
    "name": "TobiLG",
    "email": "tobilg@gmail.com",
    "url": "https://github.com/tobilg"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobilg/azure-edge-locations/issues"
  },
  "homepage": "https://github.com/tobilg/azure-edge-locations#readme",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.7.0",
    "chai": "^6.2.0",
    "mocha": "^11.7.4",
    "puppeteer": "^24.23.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
